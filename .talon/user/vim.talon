delete: "dd"
delete <number>: "{number}dd"

write:
  ":w"
  key(enter)

words: " w"
up: " k"
down: " j"
start: "0"
end: "$"

top: "gg"
bottom: "G"
center: "zz"

terminal copy: key(ctrl-shift-c)
terminal paste: key(ctrl-shift-v)

escape: key(escape)
insert: "i"
append: "A"
visual: key(shift-v)
yank: "y"
yank line: "yy"
yank start: "y0"
yank end: "y$"
paste: "p"
paste before: "P"
undo: "u"
